+        &MFVideoFormat_RGB24,
+    },
+    {
+        &MFMediaType_Video,
+        &MFVideoFormat_RGB565,
+    },
+    {
+        &MFMediaType_Video,
+        &MFVideoFormat_RGB555,
+    },
+    {
+        &MFMediaType_Video,
+        &MFVideoFormat_RGB8,
+    },
+};
